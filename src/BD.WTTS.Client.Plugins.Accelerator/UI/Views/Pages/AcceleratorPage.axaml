<spp:PageBase
    x:Class="BD.WTTS.UI.Views.Pages.AcceleratorPage"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="using:Avalonia.Xaml.Interactivity"
    xmlns:ia="using:Avalonia.Xaml.Interactions.Core"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ms="https://steampp.net/settings"
    xmlns:s="https://steampp.net/services"
    xmlns:spp="https://steampp.net/ui"
    xmlns:ui="using:FluentAvalonia.UI.Controls"
    Title="{Binding Name, Source={x:Static s:Plugin.Instance}, Mode=OneWay}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:CompileBindings="True"
    x:DataType="spp:AcceleratorPageViewModel"
    Description="{Binding Description, Source={x:Static s:Plugin.Instance}, Mode=OneWay}"
    OpenPaneLength="335"
    ScrollViewer.VerticalScrollBarVisibility="Disabled"
    Subtitle="{Binding Author, Source={x:Static s:Plugin.Instance}, Mode=OneWay, Converter={StaticResource StringFormatConverter}, ConverterParameter=Plugin_Author}"
    mc:Ignorable="d">
    <spp:PageBase.PreviewImage>
        <ui:ImageIconSource Source="{Binding Icon, Source={x:Static s:Plugin.Instance}, Mode=OneWay, Converter={StaticResource BitmapAssetValueConverter}}" />
    </spp:PageBase.PreviewImage>
    <spp:PageBase.ActionContent>
        <StackPanel Orientation="Horizontal" Spacing="2">
            <Button
                Padding="8,4"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Command="{Binding StartProxyCommand}"
                DockPanel.Dock="Right"
                IsVisible="{Binding Source={x:Static s:ProxyService.Current}, Path=!ProxyStatus, Mode=OneWay}"
                Theme="{StaticResource TransparentButton}">
                <!--<SplitButton.Flyout>
                    <ui:FAMenuFlyout>
                        <ui:MenuFlyoutSubItem ItemsSource="{Binding Source={x:Static ms:ProxySettings.ProxyModes}, Path=., Mode=OneWay}" Text="加速模式">
                            <ui:MenuFlyoutSubItem.ItemTemplate>
                                <DataTemplate>
                                    <ui:RadioMenuFlyoutItem GroupName="ProxyMode" Text="{Binding}" />
                                </DataTemplate>
                            </ui:MenuFlyoutSubItem.ItemTemplate>
                        </ui:MenuFlyoutSubItem>
                        <ui:ToggleMenuFlyoutItem Text="启用脚本" />
                    </ui:FAMenuFlyout>
                </SplitButton.Flyout>-->
                <Panel>
                    <Viewbox
                        Width="16"
                        Height="16"
                        HorizontalAlignment="Left">
                        <ui:FontIcon FontFamily="{StaticResource SymbolThemeFontFamily}" Glyph="{StaticResource SpeedHigh}" />
                    </Viewbox>
                    <TextBlock
                        Margin="28,4,0,4"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Text="{Binding Path=Res.CommunityFix_StartProxy, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_StartProxy}}" />
                </Panel>
            </Button>

            <Button
                Padding="8,4"
                Command="{Binding StartProxyCommand}"
                DockPanel.Dock="Right"
                IsVisible="{Binding Source={x:Static s:ProxyService.Current}, Path=ProxyStatus, Mode=OneWay}"
                Theme="{StaticResource TransparentButton}">
                <Panel>
                    <Viewbox
                        Width="16"
                        Height="16"
                        HorizontalAlignment="Left">
                        <ui:SymbolIcon Symbol="StopFilled" />
                    </Viewbox>
                    <TextBlock
                        Margin="28,4,0,4"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center">
                        <!--<Run Text="{Binding Path=Res.CommunityFix_StopProxy,Source={x:Static spp:Strings.CommunityFix_StopProxy}},Mode=OneWay}" />
                        <Run Text=" " />
                        <Run Text="{Binding Path=AccelerateTime,Source={x:Static s:ProxyService.Current}},Mode=OneWay,Converter={StaticResource StringFormatConverter}}" />-->
                        <TextBlock.Text>
                            <MultiBinding StringFormat="{}{0} {1}">
                                <CompiledBinding
                                    Mode="OneWay"
                                    Path="Res.CommunityFix_StopProxy"
                                    Source="{x:Static spp:Strings.CommunityFix_StopProxy}}" />
                                <CompiledBinding
                                    Converter="{StaticResource StringFormatConverter}"
                                    Mode="OneWay"
                                    Path="AccelerateTime"
                                    Source="{x:Static s:ProxyService.Current}" />
                            </MultiBinding>
                        </TextBlock.Text>
                    </TextBlock>
                </Panel>
            </Button>

            <Separator
                Height="18"
                Margin="5,0"
                VerticalAlignment="Center"
                Background="{DynamicResource ButtonBorderBrush}"
                Classes="Vertical"
                DockPanel.Dock="Right"
                IsHitTestVisible="False" />


            <Button
                Padding="8,4"
                Command="{Binding RefreshCommand}"
                DockPanel.Dock="Right"
                Theme="{StaticResource TransparentButton}">
                <Panel>
                    <Viewbox
                        Width="16"
                        Height="16"
                        HorizontalAlignment="Left">
                        <ui:SymbolIcon Symbol="Refresh" />
                    </Viewbox>
                    <TextBlock
                        Margin="28,4,0,4"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Text="{Binding Path=Res.Refresh, Mode=OneWay, Source={x:Static spp:Strings.Refresh}}" />
                </Panel>
            </Button>

            <ToggleButton
                Padding="8,4"
                DockPanel.Dock="Right"
                IsChecked="{Binding $parent[spp:PageBase].IsPaneOpen, Mode=TwoWay}"
                Theme="{StaticResource TransparentToggleButton}">
                <Panel>
                    <Viewbox
                        Width="16"
                        Height="16"
                        HorizontalAlignment="Left">
                        <ui:SymbolIcon Symbol="Settings" />
                    </Viewbox>
                    <TextBlock
                        Margin="28,4,0,4"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Text="{Binding Path=Res.CommunityFix_MenuName, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_MenuName}}" />
                </Panel>
            </ToggleButton>

            <DropDownButton
                Padding="8"
                HorizontalContentAlignment="Center"
                VerticalContentAlignment="Center"
                Content="{StaticResource More}"
                FontFamily="{StaticResource SymbolThemeFontFamily}"
                FontSize="20"
                Theme="{StaticResource TransparentButton}">
                <DropDownButton.Flyout>
                    <ui:FAMenuFlyout Placement="BottomEdgeAlignedRight">
                        <!--<ui:MenuFlyoutSubItem ItemsSource="{Binding Source={x:Static ms:ProxySettings.ProxyModes}, Path=., Mode=OneWay}" Text="加速模式">
                            <ui:MenuFlyoutSubItem.ItemTemplate>
                                <DataTemplate>
                                    <ui:RadioMenuFlyoutItem GroupName="ProxyMode" Text="{Binding}" />
                                </DataTemplate>
                            </ui:MenuFlyoutSubItem.ItemTemplate>
                        </ui:MenuFlyoutSubItem>-->
                        <!--<ui:MenuFlyoutItem Text="{Binding Path=Res.Settings_Plugin, Mode=OneWay, Source={x:Static spp:Strings.Settings_Plugin}}" />-->
                        <!--<ui:MenuFlyoutSeparator />-->
                        <!--<ui:MenuFlyoutItem Text="安装证书" />
                        <ui:MenuFlyoutItem Text="移除证书" />
                        <ui:MenuFlyoutItem Text="打开证书文件夹" />
                        <ui:MenuFlyoutSeparator />
                        <ui:MenuFlyoutItem Text="编辑 hosts 文件" />
                        <ui:MenuFlyoutItem Text="重置 hosts 文件" />
                        <ui:MenuFlyoutItem Text="打开 hosts 文件夹" />
                        <ui:MenuFlyoutSeparator />-->
                        <ui:MenuFlyoutItem IsEnabled="False">
                            <ui:MenuFlyoutItem.Text>
                                <MultiBinding StringFormat="{}{0}{1}">
                                    <CompiledBinding
                                        Mode="OneWay"
                                        Path="Res.Plugin_Version"
                                        Source="{x:Static spp:Strings.Plugin_Version}" />
                                    <CompiledBinding
                                        Mode="OneWay"
                                        Path="Version"
                                        Source="{x:Static s:Plugin.Instance}" />
                                </MultiBinding>
                            </ui:MenuFlyoutItem.Text>
                        </ui:MenuFlyoutItem>
                        <!--<ui:MenuFlyoutItem Text="{Binding Path=Res.ViewInStore, Mode=OneWay, Source={x:Static spp:Strings.ViewInStore}}" />
                        <ui:MenuFlyoutItem Text="{Binding Path=Res.UseHelp, Mode=OneWay, Source={x:Static spp:Strings.UseHelp}}" />
                        <ui:MenuFlyoutItem Text="{Binding Path=Res.AboutThisPlugin, Mode=OneWay, Source={x:Static spp:Strings.AboutThisPlugin}}" />-->
                    </ui:FAMenuFlyout>
                </DropDownButton.Flyout>
            </DropDownButton>
        </StackPanel>
    </spp:PageBase.ActionContent>
    <spp:PageBase.PaneContent>
        <StackPanel Margin="15,20" Spacing="10">
            <TextBlock Text="{Binding Path=Res.CommunityFix_ProxyMode, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_ProxyMode}}" Theme="{StaticResource BodyStrongTextBlockStyle}" />

            <TabStrip
                Background="{DynamicResource ControlOnImageFillColorDefaultBrush}"
                CornerRadius="{StaticResource ControlCornerRadius}"
                IsEnabled="{Binding Source={x:Static s:ProxyService.Current}, Path=!ProxyStatus, Mode=OneWay}"
                ItemContainerTheme="{StaticResource TabItemStyle}"
                ItemsSource="{Binding Source={x:Static ms:ProxySettings.ProxyModes}, Path=., Mode=OneWay}"
                SelectedItem="{Binding Source={x:Static ms:ProxySettings.ProxyMode}, Path=Value, Mode=TwoWay}"
                Theme="{StaticResource TabStripStyle}">
                <TabStrip.ItemTemplate>
                    <DataTemplate DataType="spp:ProxyMode">
                        <TextBlock Text="{Binding}" />
                    </DataTemplate>
                </TabStrip.ItemTemplate>
            </TabStrip>

            <TextBlock Text="{Binding Path=Res.CommunityFix_MenuName, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_MenuName}}" Theme="{StaticResource BodyStrongTextBlockStyle}" />
            <DockPanel>
                <ToggleSwitch
                    VerticalAlignment="Center"
                    Classes="NoContent"
                    IsChecked="{Binding Source={x:Static s:ProxyService.Current}, Path=IsEnableScript, Mode=TwoWay}"
                    OffContent="{Binding Path=Res.CommunityFix_EnableScriptService, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_EnableScriptService}}"
                    OnContent="{Binding Path=Res.CommunityFix_EnableScriptService, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_EnableScriptService}}" />

                <!--<WrapPanel>-->
                <TextBlock
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Classes.NotSupport="{Binding Source={x:Static s:ProxyService.Current}, Path=IPv6AddresString, Mode=OneWay, Converter={StaticResource IsNullConverter}}"
                    Classes.Support="{Binding Source={x:Static s:ProxyService.Current}, Path=IPv6AddresString, Mode=OneWay, Converter={StaticResource IsNullConverter}, ConverterParameter=invert}"
                    Theme="{StaticResource CaptionTextBlockStyle}"
                    ToolTip.Tip="{Binding Source={x:Static s:ProxyService.Current}, Path=IPv6AddresString, Mode=OneWay}">
                    <TextBlock.Styles>
                        <Style Selector="TextBlock.Support">
                            <Setter Property="Text" Value="已支持 IPv6 访问" />
                            <Setter Property="Foreground" Value="{DynamicResource SystemFillColorSuccessBrush}" />
                        </Style>
                        <Style Selector="TextBlock.NotSupport">
                            <Setter Property="Text" Value="不支持 IPv6 访问" />
                            <Setter Property="Foreground" Value="{DynamicResource SystemFillColorCriticalBrush}" />
                        </Style>
                    </TextBlock.Styles>
                </TextBlock>
                <!--<Button
						VerticalAlignment="Center"
						Command="{Binding Source={x:Static s:ProxyService.Current}, Path=RefreshIpv6Support, Mode=OneWay}"
						Theme="{StaticResource TransparentButton}"
						ToolTip.Tip="{Binding Path=Res.Refresh, Mode=OneWay, Source={x:Static spp:Strings.Refresh}}">
						<ui:SymbolIcon Symbol="Refresh" />
					</Button>
				</WrapPanel>-->
            </DockPanel>
            <ToggleSwitch
                VerticalAlignment="Center"
                Classes="NoContent"
                IsChecked="{Binding Source={x:Static ms:ProxySettings.ProgramStartupRunProxy}, Path=Value, Mode=TwoWay}"
                OffContent="{Binding Path=Res.CommunityFix_AutoRunProxy, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_AutoRunProxy}}"
                OnContent="{Binding Path=Res.CommunityFix_AutoRunProxy, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_AutoRunProxy}}" />

            <!--<StackPanel Orientation="Horizontal" Spacing="4">
                <ToggleButton
                    Padding="8"
                    Theme="{StaticResource TransparentToggleButton}"
                    ToolTip.Tip="{Binding Path=Res.GameList_OnlyInstalled,
                                                    Mode=OneWay,
                                                    Source={x:Static spp:Strings.GameList_OnlyInstalled}}">
                    <Viewbox Width="20" Height="20">
                        <ui:SymbolIcon Symbol="Play" />
                    </Viewbox>
                </ToggleButton>

                <ToggleButton
                    Padding="8"
                    Theme="{StaticResource TransparentToggleButton}"
                    ToolTip.Tip="{Binding Path=Res.GameList_OnlyCloudArchive,
                                                    Mode=OneWay,
                                                    Source={x:Static spp:Strings.GameList_OnlyCloudArchive}}">
                    <Viewbox Width="20" Height="20">
                        <ui:SymbolIcon Symbol="CloudSyncComplete" />
                    </Viewbox>
                </ToggleButton>
            </StackPanel>-->

            <Button
                HorizontalAlignment="Stretch"
                Command="{Binding ProxySettingsCommand}"
                Content="{Binding Path=Res.CommunityFix_ProxySettings, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_ProxySettings}}" />

            <Separator Margin="0,8" Classes="Horizontal" />

            <Button
                HorizontalAlignment="Stretch"
                Command="{Binding SetupCertificateCommand}"
                Content="{Binding Path=Res.CommunityFix_SetupCertificate, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_SetupCertificate}}" />
            <Button
                HorizontalAlignment="Stretch"
                Command="{Binding DeleteCertificateCommand}"
                Content="{Binding Path=Res.CommunityFix_DeleteCertificate, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_DeleteCertificate}}" />
            <Button
                HorizontalAlignment="Stretch"
                Command="{Binding OpenCertificateDirCommand}"
                Content="{Binding Path=Res.CommunityFix_OpenCertificateDir, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_OpenCertificateDir}}" />

            <Separator Margin="0,8" Classes="Horizontal" />

            <Button
                HorizontalAlignment="Stretch"
                Command="{Binding EditHostsFileCommand}"
                Content="{Binding Path=Res.CommunityFix_EditHostsFile, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_EditHostsFile}}" />
            <Button
                HorizontalAlignment="Stretch"
                Command="{Binding ResetHostsFileCommand}"
                Content="{Binding Path=Res.CommunityFix_ResetHostsFile, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_ResetHostsFile}}" />
            <Button
                HorizontalAlignment="Stretch"
                Command="{Binding OpenHostsDirCommand}"
                Content="{Binding Path=Res.CommunityFix_OpenHostsDir, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_OpenHostsDir}}" />

            <!--<ui:FAComboBox ItemsSource="{Binding Source={x:Static ms:ProxySettings.ProxyModes}, Path=., Mode=OneWay}" SelectedItem="{Binding Source={x:Static ms:ProxySettings.ProxyMode}, Path=Value, Mode=TwoWay}">
                <ui:FAComboBox.ItemTemplate>
                    <DataTemplate DataType="spp:ProxyMode">
                        <TextBlock Text="{Binding}" />
                    </DataTemplate>
                </ui:FAComboBox.ItemTemplate>
            </ui:FAComboBox>-->

        </StackPanel>
    </spp:PageBase.PaneContent>
    <Grid ColumnDefinitions="*,Auto">
        <ScrollViewer
            Padding="0,0,10,0"
            HorizontalScrollBarVisibility="Disabled"
            VerticalScrollBarVisibility="Auto">
            <Carousel
                Name="AcceleratorCarousel"
                Classes="AnimeSlide"
                SelectedIndex="{Binding Source={x:Static s:ProxyService.Current}, Path=ProxyStatus, Mode=OneWay}">
                <!--<i:Interaction.Behaviors>
                    <ia:DataTriggerBehavior
                        Binding="{Binding Source={x:Static s:ProxyService.Current}, Path=!ProxyStatus, Mode=OneWay}"
                        ComparisonCondition="Equal"
                        Value="True">
                        <ia:ChangePropertyAction
                            PropertyName="SelectedIndex"
                            TargetObject="{Binding #AcceleratorCarousel}"
                            Value="0" />
                    </ia:DataTriggerBehavior>
                    <ia:DataTriggerBehavior
                        Binding="{Binding Source={x:Static s:ProxyService.Current}, Path=!ProxyStatus, Mode=OneWay}"
                        ComparisonCondition="Equal"
                        Value="False">
                        <ia:ChangePropertyAction
                            PropertyName="SelectedIndex"
                            TargetObject="{Binding #AcceleratorCarousel}"
                            Value="1" />
                    </ia:DataTriggerBehavior>
                </i:Interaction.Behaviors>-->

                <spp:ContentLoader IsLoading="{Binding Source={x:Static s:ProxyService.Current}, Path=!!!ProxyDomainsList.Count, Mode=OneWay}"
								   NoResultMessage="{Binding Path=Res.CommunityFix_ItemsEmpty, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_ItemsEmpty}}">
                    <Grid ColumnDefinitions="*,Auto">
                        <ItemsControl ItemsSource="{Binding Source={x:Static s:ProxyService.Current}, Path=ProxyDomainsList, Mode=OneWay}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Vertical" Spacing="{StaticResource SettingsSpacing}" />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>

                            <ItemsControl.ItemTemplate>
                                <DataTemplate x:DataType="spp:AccelerateProjectGroupDTO">
                                    <Border>
                                        <!--<ui:SettingsExpander ItemsSource="{Binding Items}">-->
                                        <ui:SettingsExpander IsClickEnabled="False">
                                            <ui:SettingsExpander.Header>
                                                <StackPanel Orientation="Horizontal" Spacing="10">
                                                    <CheckBox IsChecked="{Binding ThreeStateEnable, Mode=TwoWay}" />
                                                    <spp:Image2
                                                        Width="30"
                                                        Height="30"
                                                        VerticalAlignment="Center"
                                                        RenderOptions.BitmapInterpolationMode="HighQuality"
                                                        Source="{Binding IconUrl}"
                                                        Stretch="UniformToFill" />
                                                    <TextBlock VerticalAlignment="Center" Text="{Binding Name}" />
                                                </StackPanel>
                                            </ui:SettingsExpander.Header>
                                            <ui:SettingsExpanderItem Theme="{StaticResource SettingsExpanderItem2}">
                                                <TreeView ItemsSource="{Binding ObservableItems}" SelectionMode="Single">
                                                    <TreeView.ItemTemplate>
                                                        <TreeDataTemplate DataType="spp:AccelerateProjectDTO" ItemsSource="{Binding ObservableItems}">
                                                            <DockPanel Margin="8,0">
                                                                <CheckBox
                                                                    HorizontalAlignment="Left"
                                                                    DockPanel.Dock="Left"
                                                                    IsChecked="{Binding ThreeStateEnable, Mode=TwoWay}">
                                                                    <TextBlock VerticalAlignment="Center" Text="{Binding Name}" />
                                                                </CheckBox>
                                                                <TextBlock
                                                                    HorizontalAlignment="Right"
                                                                    VerticalAlignment="Center"
                                                                    DockPanel.Dock="Right"
                                                                    Text="{Binding DomainNamesArray[0]}" />
                                                            </DockPanel>
                                                        </TreeDataTemplate>
                                                    </TreeView.ItemTemplate>
                                                </TreeView>
                                            </ui:SettingsExpanderItem>
                                            <!--<ui:SettingsExpander.ItemTemplate>
                                        <DataTemplate DataType="spp:AccelerateProjectDTO">
                                            <ui:SettingsExpanderItem IsClickEnabled="True">
                                                <ui:SettingsExpanderItem.Content>
                                                    <TextBlock Text="{Binding Name}" />
                                                </ui:SettingsExpanderItem.Content>
                                                <ui:SettingsExpanderItem.IconSource>
                                                    <ui:SymbolIconSource Symbol="Accept" />
                                                </ui:SettingsExpanderItem.IconSource>
                                                <ui:SettingsExpanderItem.Footer>
                                                    <TextBlock Text="{Binding DomainNamesArray[0]}" />
                                                </ui:SettingsExpanderItem.Footer>
                                            </ui:SettingsExpanderItem>
                                        </DataTemplate>
                                    </ui:SettingsExpander.ItemTemplate>-->
                                        </ui:SettingsExpander>
                                    </Border>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>

                        </ItemsControl>
                    </Grid>
                </spp:ContentLoader>

                <Grid ColumnDefinitions="*,Auto" RowDefinitions="Auto,*,Auto">
                    <TabStrip
                        x:Name="Tabs"
                        ItemContainerTheme="{StaticResource TabItemStyle}"
                        Theme="{StaticResource TabStripStyle}">
                        <TabStripItem Content="{Binding Path=Res.CommunityFix_FlowStatistics, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_FlowStatistics}}" />
                        <TabStripItem Content="{Binding Path=Res.CommunityFix_Services, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_Services}}" />
                    </TabStrip>
                    <StackPanel
                        Grid.Row="0"
                        Grid.Column="1"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Spacing="8">
                        <StackPanel.Styles>
                            <Style Selector="TextBlock">
                                <Setter Property="VerticalAlignment" Value="Center" />
                            </Style>
                        </StackPanel.Styles>
                        <!--<StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                        <TextBlock FontSize="26" Text="{Binding Path=Res.CommunityFix_AlreadyProxy, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_AlreadyProxy}}" />
                        <TextBlock
                            Margin="5,0,0,0"
                            FontSize="26"
                            Text="{Binding AccelerateTime,
                                                     Source={x:Static s:ProxyService.Current},
                                                     StringFormat={}{0:hh\\:mm\\:ss}}" />
                    </StackPanel>-->
                        <TextBlock>
                            <Run Text="{Binding Path=Res.CommunityFix_ProxyMode, StringFormat={}{0}：, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_ProxyMode, StringFormat={}{0}：}}" />
                            <Run Text="{Binding Source={x:Static ms:ProxySettings.ProxyMode}, Path=Value, Mode=OneWay, Converter={StaticResource EnumLocalizationNameConverter}}" />

                            <Run Text="  " />

                            <Run Text="{Binding Path=Res.CommunityFix_EnableScriptService, StringFormat={}{0}：, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_EnableScriptService, StringFormat={}{0}：}}" />
                            <Run Text="{Binding Source={x:Static s:ProxyService.Current}, Path=IsEnableScript, Converter={StaticResource StringFormatConverter}}" />
                        </TextBlock>
                    </StackPanel>

                    <Carousel
                        Grid.Row="1"
                        Grid.ColumnSpan="2"
                        Classes="AnimeSlide"
                        SelectedIndex="{Binding #Tabs.SelectedIndex}">

                        <spp:ProxyChartView Margin="5,0" />

                        <Grid ColumnDefinitions="*,20,*">
                            <Border
                                Grid.Column="0"
                                Padding="10"
                                Classes="paper">
                                <DockPanel>
                                    <TextBlock
                                        Margin="0,0,0,10"
                                        DockPanel.Dock="Top"
                                        FontSize="16"
                                        Text="{Binding Path=Res.CommunityFix_AccelerationsEnable, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_AccelerationsEnable}}" />
                                    <ScrollViewer>
                                        <ItemsControl ItemsSource="{Binding EnableProxyDomains, Source={x:Static s:ProxyService.Current}}">
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate DataType="{x:Type spp:AccelerateProjectDTO}">
                                                    <StackPanel Margin="10">
                                                        <DockPanel>
                                                            <TextBlock Text="{Binding Name}" />
                                                        </DockPanel>
                                                        <Separator Classes="Separator" />
                                                    </StackPanel>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>
                                        </ItemsControl>
                                    </ScrollViewer>
                                </DockPanel>
                            </Border>

                            <Border
                                Grid.Column="2"
                                Padding="10"
                                Classes="paper">
                                <DockPanel>
                                    <TextBlock
                                        Margin="0,0,0,10"
                                        DockPanel.Dock="Top"
                                        FontSize="16"
                                        Text="{Binding Path=Res.CommunityFix_ScriptsEnable, Mode=OneWay, Source={x:Static spp:Strings.CommunityFix_ScriptsEnable}}" />
                                    <ScrollViewer>
                                        <ItemsControl ItemsSource="{Binding EnableProxyScripts^, Source={x:Static s:ProxyService.Current}}">
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate DataType="{x:Type spp:ScriptDTO}">
                                                    <StackPanel Margin="10">
                                                        <DockPanel>
                                                            <TextBlock Text="{Binding Name}" />
                                                        </DockPanel>
                                                        <Separator Classes="Separator" />
                                                    </StackPanel>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>
                                        </ItemsControl>
                                    </ScrollViewer>
                                </DockPanel>
                            </Border>
                        </Grid>


                        <!--<TabItem Header="加速日志">

					    </TabItem>-->
                        <!--<TabItem Header="AD">
					
						</TabItem>-->
                    </Carousel>
                </Grid>
            </Carousel>
        </ScrollViewer>

        <spp:AdControl
            Grid.Column="1"
            Width="165"
            Height="390"
            VerticalAlignment="Top"
            Standard="Vertical" />
    </Grid>
</spp:PageBase>
